{
  "expo": {
    "name": "Excellia",
    "slug": "excellia-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oussema22.excelliamobile",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.oussema22.excelliamobile",
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0f172a"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "./withCleartextTraffic",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f172a"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Excellia to use your location for attendance check-in."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "36a6e191-6da1-444d-a42d-9b3590e15d34"
      }
    }
  }
}